<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sdu.comsci.orawon.bubbletea.additional">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar9"
        android:layout_width="433dp"
        android:layout_height="wrap_content"
        android:background="@color/barmenu"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button19"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="70dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/listmenu"
        android:onClick="onClickhomemenu"
        android:textColorLink="@android:color/holo_red_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar9"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="62dp"
        android:layout_marginStart="62dp"
        android:text="เมนูร้าน"
        android:textColor="@android:color/black"
        android:textSize="@dimen/h4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button19"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginTop="0dp" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar8"
        android:layout_width="387dp"
        android:layout_height="70dp"
        android:background="@color/linkuser"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/button21"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="166dp"
        android:layout_marginStart="166dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/listorder"
        android:onClick="onClickorderlist"
        android:textColorLink="@android:color/holo_red_light"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintTop_toTopOf="@+id/toolbar9" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:layout_marginLeft="141dp"
        android:layout_marginStart="141dp"
        android:text="รายการสั่งซื้อ"
        android:textColor="@android:color/black"
        android:textSize="@dimen/h4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button21"
        app:layout_constraintVertical_bias="0.111"
        android:layout_marginTop="0dp" />

    <Button
        android:id="@+id/button22"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="271dp"
        android:layout_marginStart="271dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/point"
        android:onClick="onClickother"
        android:textColorLink="@android:color/holo_red_light"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar9"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:layout_marginLeft="262dp"
        android:layout_marginStart="262dp"
        android:text="เพิ่มเติม"
        android:textColor="@android:color/black"
        android:textSize="@dimen/h4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button22"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginTop="0dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="เพิ่มเติม"
        android:textColor="@android:color/white"
        android:textSize="@dimen/h6"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar8"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="383dp"
        android:layout_height="178dp"
        app:srcCompat="@drawable/pictitle"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar8"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button12"
        android:layout_width="365dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/tabbb"
        android:onClick="onClickreturnregister"
        android:text="ลงชื่อเข้าใช้ หรือ สร้างบัญชีผู้ใช้ใหม่"
        android:textColor="@android:color/black"
        android:textColorLink="@color/link"
        android:textSize="@dimen/h4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/button17"
        android:layout_width="365dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/tabbb"
        android:onClick="onClickbranch"
        android:text="สาขาใกล้เคียง"
        android:textColor="@android:color/black"
        android:textColorLink="@color/link"
        android:textSize="@dimen/h4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button12" />

    <Button
        android:id="@+id/button18"
        android:layout_width="365dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/tabbb"
        android:onClick="onClickaboutshop"
        android:text="เกี่ยวกับเรา"
        android:textColor="@android:color/black"
        android:textColorLink="@color/link"
        android:textSize="@dimen/h4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button17" />
</android.support.constraint.ConstraintLayout>
